<Window x:Class="MidiToJoy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MidiToJoy"
        mc:Ignorable="d"
        Title="MidiToJoy" Loaded="Window_Loaded" SizeToContent="WidthAndHeight" Closed="Window_Closed">
	<StackPanel Margin="5">
		<ComboBox x:Name="comboBoxMidiInDevices" SelectionChanged="ComboBoxMidiInDevices_SelectionChanged"></ComboBox>
		<TextBlock x:Name="textBox"></TextBlock>
	</StackPanel>
</Window>
